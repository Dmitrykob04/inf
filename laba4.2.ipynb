      "source": [
        "k = int(input('Введите число элементов\\n'))\n",
        "\n",
        "from random import randint\n",
        "A = [0]*k\n",
        "for i in range(k):\n",
        "\tA[i] = randint(1,9)\n",
        "print(A)\n",
        "\n",
        "def func_2(L):\n",
        "\tB = []\n",
        "\tfor j in range(1, k - 1):\n",
        "\t\tif L[j] > L[j-1]:\n",
        "\t\t\tB.append(L[j])\n",
        "\treturn(print(B))\n",
        "\t\n",
        "func_2(A)"
      ]
